<body>
    <script>
        function saltarLinea() {
            document.write("<br>");    
            document.write("<br>");    
            document.write("<br>");    
        }  
        function imprimir(frase) {
            document.write(frase);
            saltarLinea();
        }

    pesoGaston = 73
    alturaGaston = 1.78
    IMCGaston = pesoGaston / (alturaGaston * alturaGaston);

imprimir("El IMC de Gastón es: " + IMCGaston)
    </script>
